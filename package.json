{
  "name": "privatekit",
  "version": "0.9.1",
  "private": true,
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "install:pod": "cd ios && pod install",
    "postinstall": "patch-package; yarn react-native-jetifier",
    "postversion": "react-native-version",
    "format:all": "prettier --write ./app/**/*.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint-fix": "eslint --fix .",
    "lint-check": "eslint .",
    "i18n:extract": "i18next",
    "test": "jest test"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "rnpm": {
    "assets": [
      "app/assets/fonts"
    ]
  },
  "dependencies": {
    "@bam.tech/react-native-component-map-clustering": "^1.1.1",
    "@mauron85/react-native-background-geolocation": "^0.6.3",
